                subText.Transparency = i
                bg.Transparency = 0.3 + i * 0.7
                task.wait(0.01)
            end
            
            -- ç§»é™¤ç»˜å›¾å¯¹è±¡
            bg:Remove()
            text:Remove()
            subText:Remove()
        end)
    end

