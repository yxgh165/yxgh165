local function CopyToClipboard(text)
    if setclipboard then
        setclipboard(text)
        return true
    elseif writeclipboard then
        writeclipboard(text)
        return true
    elseif toclipboard then
        toclipboard(text)
        return true
    else
        print("[å¤åˆ¶å†…å®¹] " .. text)
        return false
    end
end

