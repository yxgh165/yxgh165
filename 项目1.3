    subText.Position = Vector2.new(workspace.CurrentCamera.ViewportSize.X/2, -100) -- åˆå§‹ä½ç½®åœ¨å±å¹•ä¸Šæ–¹
    subText.Color = Color3.new(0.8, 0.8, 1)
    subText.Outline = true
