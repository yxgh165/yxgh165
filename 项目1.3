        -- æ€»åœç•™æ—¶é—´5ç§’ (ä»åŠ¨ç”»å¼€å§‹è®¡ç®—)
        wait(3.5) -- å› ä¸ºåŠ¨ç”»å·²ç»ç”¨äº†1.5ç§’
        
        -- æ·¡å‡ºåŠ¨ç”»
        for i = 0, 1, 0.05 do
            text.Transparency = i
            subText.Transparency = i
            bg.Transparency = 0.3 + i * 0.7
            wait(0.01)
        end
        
        -- ç§»é™¤ç»˜å›¾å¯¹è±¡
        bg:Remove()
        text:Remove()
        subText:Remove()
    end)
end

