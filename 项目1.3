        mouseSec:AddTextbox({ Name = "åŠå¾„ï¼ˆè¾“å…¥ï¼‰", Default = tostring(mouseFOV), Callback = function(txt) local n = tonumber(txt) if n then mouseFOV = n; mouseFovCircle.Radius = n; mouseSlider:Set(n) end end })
        
