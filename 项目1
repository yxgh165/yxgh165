-- ç§»åŠ¨åŠ é€Ÿ Tabï¼ˆå®Œæ•´ç‰ˆï¼Œå«å¤œè§†+é£è¡Œï¼‰
local Tab4 = Window:MakeTab({Name = "äººç‰©åŠŸèƒ½", Icon = "rbxassetid://7733779610"})

-- è·å–äººç‰©æ¨¡å—
local function getChar()
    local p = game.Players.LocalPlayer
    return p.Character or p.CharacterAdded:Wait()
end
local function getHum()
    return getChar():WaitForChild("Humanoid", 3)
end

-- åŸºç¡€è°ƒé€Ÿ
Tab4:AddTextbox({
    Name = "è¾“å…¥é€Ÿåº¦æ•°å€¼",
    Default = "16",
    Callback = function(txt)
        local hum = getHum()
        if hum then hum.WalkSpeed = tonumber(txt) or 16 end
    end
})
Tab4:AddTextbox({
    Name = "è¾“å…¥è·³è·ƒæ•°å€¼",
    Default = "50",
    Callback = function(txt)
        local hum = getHum()
        if hum then hum.JumpPower = tonumber(txt) or 50 end
    end
})
Tab4:AddTextbox({
    Name = "è¾“å…¥é‡åŠ›æ•°å€¼",
    Default = "196",
    Callback = function(txt)
        workspace.Gravity = tonumber(txt) or 196
    end
})

-- å¤œè§†å¼€å…³
Tab4:AddToggle({
    Name = "å¤œè§†æ¨¡å¼",
    Default = false,
    Callback = function(v)
        if v then
            game.Lighting.Ambient = Color3.fromRGB(255,255,255)
            game.Lighting.Brightness = 5
            game.Lighting.GlobalShadows = false
            OrionLib:MakeNotification({Name="æç¤º", Content="å¤œè§†å·²å¼€å¯", Time=2})
        else
            game.Lighting.Ambient = Color3.fromRGB(128,128,128)
            game.Lighting.Brightness = 2
            game.Lighting.GlobalShadows = true
            OrionLib:MakeNotification({Name="æç¤º", Content="å¤œè§†å·²å…³é—­", Time=2})
        end
    end
})

-- é£è¡Œå¼€å…³
local flyConn = {}
Tab4:AddToggle({
    Name = "é£è¡Œæ¨¡å¼",
    Default = false,
    Callback = function(v)
        local char = getChar()
        local hrp = char:WaitForChild("HumanoidRootPart")
        local hum = char:WaitForChild("Humanoid")
        
        -- æ¸…ç†æ—§è¿æ¥
        for _,c in ipairs(flyConn) do c:Disconnect() end
        flyConn = {}

        if v then
            hum.PlatformStand = true
            local bg = Instance.new("BodyGyro")
            bg.MaxTorque = Vector3.new(4000,4000,4000)
            bg.P = 10000
            bg.Parent = hrp

            local bv = Instance.new("BodyVelocity")
            bv.MaxForce = Vector3.new(5000,5000,5000)
            bv.Velocity = Vector3.new(0,0,0)
            bv.Parent = hrp

            local UIS = game:GetService("UserInputService")
            local speed = 50

            local function onInput(inp, gpe)
                if gpe then return end
                local vec = Vector3.new(0,0,0)
                if inp.UserInputType == Enum.UserInputType.Keyboard then
                    local key = inp.KeyCode
                    if key == Enum.KeyCode.W then vec = vec + hrp.CFrame.lookVector end
                    if key == Enum.KeyCode.S then vec = vec - hrp.CFrame.lookVector end
                    if key == Enum.KeyCode.A then vec = vec - hrp.CFrame.rightVector end
                    if key == Enum.KeyCode.D then vec = vec + hrp.CFrame.rightVector end
                    if key == Enum.KeyCode.Space then vec = vec + Vector3.new(0,1,0) end
                    if key == Enum.KeyCode.LeftShift then vec = vec - Vector3.new(0,1,0) end
                end
                bv.Velocity = vec * speed
            end

            local function onEnd(inp)
                if inp.UserInputType == Enum.UserInputType.Keyboard then
                    local key = inp.KeyCode
                    if key == Enum.KeyCode.W or key == Enum.KeyCode.S or
                       key == Enum.KeyCode.A or key == Enum.KeyCode.D or
                       key == Enum.KeyCode.Space or key == Enum.KeyCode.LeftShift then
                        bv.Velocity = Vector3.new(0,0,0)
                    end
                end
            end

            table.insert(flyConn, UIS.InputBegan:Connect(onInput))
            table.insert(flyConn, UIS.InputEnded:Connect(onEnd))

            OrionLib:MakeNotification({Name="é£è¡Œæç¤º", Content="é£è¡Œå·²å¼€å¯\nWASDç§»åŠ¨ ç©ºæ ¼ä¸Šå‡ Shiftä¸‹é™", Time=3})
        else
            hum.PlatformStand = false
            for _,obj in ipairs({"BodyGyro","BodyVelocity"}) do
                if hrp:FindFirstChild(obj) then hrp[obj]:Destroy() end
            end
            OrionLib:MakeNotification({Name="é£è¡Œæç¤º", Content="é£è¡Œå·²å…³é—­", Time=2})
        end
    end
})

-- ç©¿å¢™å¼€å…³
local noclipConn
Tab4:AddToggle({
    Name = "ç©¿å¢™æ¨¡å¼",
    Default = false,
    Callback = function(v)
        local char = getChar()
        if v then
            noclipConn = game:GetService("RunService").Stepped:Connect(function()
                for _,p in ipairs(char:GetDescendants()) do
                    if p:IsA("BasePart") then
                        p.CanCollide = false
                    end
                end
            end)
            OrionLib:MakeNotification({Name="æç¤º", Content="ç©¿å¢™å·²å¼€å¯", Time=2})
        else
            if noclipConn then noclipConn:Disconnect() end
            OrionLib:MakeNotification({Name="æç¤º", Content="ç©¿å¢™å·²å…³é—­", Time=2})
        end
    end
})

-- åˆ é™¤æ¸¸æˆé˜´å½±å¼€å…³
Tab4:AddToggle({
    Name = "åˆ é™¤é˜´å½±",
    Default = false,
    Callback = function(v)
        if v then
            game.Lighting.GlobalShadows = false
            for _,l in ipairs(workspace:GetDescendants()) do
                if l:IsA("BasePart") or l:IsA("MeshPart") then
                    l.CastShadow = false
                end
            end
            OrionLib:MakeNotification({Name="æç¤º", Content="é˜´å½±å·²åˆ é™¤", Time=2})
        else
            game.Lighting.GlobalShadows = true
            for _,l in ipairs(workspace:GetDescendants()) do
                if l:IsA("BasePart") or l:IsA("MeshPart") then
                    l.CastShadow = true
                end
            end
            OrionLib:MakeNotification({Name="æç¤º", Content="é˜´å½±å·²æ¢å¤", Time=2})
        end
    end
})

