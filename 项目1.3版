local lockSpeed, lockJump, lockGrav = false, false, false
local savedSpeed, savedJump, savedGrav = 16, 50, 196

local function applyValues()
    local char = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
    local hum = char:WaitForChild("Humanoid", 3)
    if lockSpeed then hum.WalkSpeed = savedSpeed end
    if lockJump then hum.JumpPower = savedJump end
    if lockGrav then workspace.Gravity = savedGrav end
end
game.Players.LocalPlayer.CharacterAdded:Connect(function()
    task.wait(.1)
    applyValues()
end)

-- è¾“å…¥æ¡† + å¼€å…³
local lockSec = Tab4:AddSection({Name = "ä¸‰é”å®š"})
lockSec:AddTextbox({Name = "è®¾å®šç§»é€Ÿï¼ˆä»…ä¿å­˜å€¼ï¼‰", Default = tostring(savedSpeed), Callback = function(txt) savedSpeed = tonumber(txt) or 16 end})
lockSec:AddTextbox({Name = "è®¾å®šè·³è·ƒé«˜åº¦ï¼ˆä»…ä¿å­˜å€¼ï¼‰", Default = tostring(savedJump), Callback = function(txt) savedJump = tonumber(txt) or 50 end})
lockSec:AddTextbox({Name = "è®¾å®šé‡åŠ›å€¼ï¼ˆä»…ä¿å­˜å€¼ï¼‰", Default = tostring(savedGrav), Callback = function(txt) savedGrav = tonumber(txt) or 196 end})

lockSec:AddToggle({Name = "é”å®šç§»é€Ÿ", Default = false, Callback = function(v) lockSpeed = v; applyValues() end})
lockSec:AddToggle({Name = "é”å®šè·³è·ƒ", Default = false, Callback = function(v) lockJump = v; applyValues() end})
lockSec:AddToggle({Name = "é”å®šé‡åŠ›", Default = false, Callback = function(v) lockGrav = v; applyValues() end})

(function()
    -- ... ä¹‹å‰çš„ä»£ç ä¿æŒä¸å˜ ...

    -----------------------------------------------------------------
    -- â‘ª é«˜çº§ç§»åŠ¨åŠ é€Ÿç³»ç»Ÿï¼ˆæ”¯æŒèº²é¿æœåŠ¡å™¨æ¸¸æˆï¼‰
    -----------------------------------------------------------------
    local speedEnabled = false
    local speedMultiplier = 1.5
    local speedMode = "åœ°é¢åŠ é€Ÿ" -- å¯é€‰: "åœ°é¢åŠ é€Ÿ", "å…¨å±€åŠ é€Ÿ", "ç¬ç§»å†²åˆº", "è‡ªåŠ¨è·³è·ƒ"
    local antiSlide = true -- é˜²æ­¢å¢™å£æ»‘åŠ¨
    local speedParticles = true -- é€Ÿåº¦ç²’å­æ•ˆæœ
    local speedTrail = true -- ç§»åŠ¨è½¨è¿¹æ•ˆæœ
    local speedKey = Enum.KeyCode.LeftShift -- åŠ é€Ÿå¿«æ·é”®
    local speedConn
    local lastPosition = Vector3.new(0, 0, 0)
    
    -- ç²’å­æ•ˆæœç¼“å­˜
    local particleCache = {}
    local trailCache = {}
    
    -- é€Ÿåº¦æ•ˆæœåˆ›å»ºå‡½æ•°
    local function createSpeedEffects()
        if not speedParticles and not speedTrail then return end
        
        local char = LocalPlayer.Character
        if not char then return end
        
        local root = char:FindFirstChild("HumanoidRootPart")
        if not root then return end
        
        -- åˆ›å»ºç²’å­æ•ˆæœ
        if speedParticles and not particleCache[root] then
            local particles = Instance.new("ParticleEmitter")
            particles.Name = "SpeedParticles"
            particles.LightEmission = 0.8
            particles.LightInfluence = 0
            particles.Size = NumberSequence.new({
                NumberSequenceKeypoint.new(0, 0.5),
                NumberSequenceKeypoint.new(1, 0)
            })
            particles.Texture = "rbxassetid://242487987"
            particles.Transparency = NumberSequence.new(0.5)
            particles.ZOffset = 1
            particles.Acceleration = Vector3.new(0, -5, 0)
            particles.Lifetime = NumberRange.new(0.5, 1)
            particles.Rate = 50
            particles.Rotation = NumberRange.new(0, 360)
            particles.Speed = NumberRange.new(2)
            particles.VelocitySpread = 180
            particles.Parent = root
            
            particleCache[root] = particles
        end
        
        -- åˆ›å»ºç§»åŠ¨è½¨è¿¹
        if speedTrail and not trailCache[root] then
            local trail = Instance.new("Trail")
            trail.Name = "SpeedTrail"
            trail.FaceCamera = true
            trail.LightEmission = 0.7
            trail.Color = ColorSequence.new(Color3.fromRGB(0, 200, 255))
            trail.Transparency = NumberSequence.new(0.5)
            trail.WidthScale = NumberSequence.new(0.3)
            trail.Attachment0 = Instance.new("Attachment", root)
            trail.Attachment0.Name = "SpeedTrailAttachment0"
            trail.Attachment0.WorldPosition = root.Position
            trail.Attachment1 = Instance.new("Attachment", root)
            trail.Attachment1.Name = "SpeedTrailAttachment1"
            trail.Attachment1.WorldPosition = root.Position + Vector3.new(0, 0.5, 0)
            trail.Parent = root
            
            trailCache[root] = trail
        end
    end
    
    -- æ¸…ç†æ•ˆæœ
    local function cleanSpeedEffects()
        for _, particles in pairs(particleCache) do
            pcall(function() particles:Destroy() end)
        end
        for _, trail in pairs(trailCache) do
            pcall(function() trail:Destroy() end)
        end
        table.clear(particleCache)
        table.clear(trailCache)
    end
    
    -- é€Ÿåº¦è®¡ç®—å‡½æ•°ï¼ˆå¸¦é˜²æ»‘å¢™ï¼‰
    local function calculateSpeed(deltaTime)
        local char = LocalPlayer.Character
        if not char then return end
        
        local humanoid = char:FindFirstChildOfClass("Humanoid")
        local root = char:FindFirstChild("HumanoidRootPart")
        if not humanoid or not root then return end
        
        -- æ£€æµ‹æ˜¯å¦åœ¨å¢™ä¸Šï¼ˆé˜²æ»‘å¢™ï¼‰
        local onWall = false
        if antiSlide then
            local ray = Ray.new(root.Position, root.CFrame.LookVector * 3)
            local hit, position = workspace:FindPartOnRay(ray, char)
            if hit then
                onWall = true
            end
        end
        
        -- è®¡ç®—æ–¹å‘å‘é‡
        local moveDirection = Vector3.new(0, 0, 0)
        if UserInputService:IsKeyDown(Enum.KeyCode.W) then
            moveDirection = moveDirection + root.CFrame.LookVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.S) then
            moveDirection = moveDirection - root.CFrame.LookVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.A) then
            moveDirection = moveDirection - root.CFrame.RightVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.D) then
            moveDirection = moveDirection + root.CFrame.RightVector
        end
        
        -- æ ‡å‡†åŒ–æ–¹å‘å‘é‡
        if moveDirection.Magnitude > 0 then
            moveDirection = moveDirection.Unit
        end
        
        -- åº”ç”¨ä¸åŒåŠ é€Ÿæ¨¡å¼
        if speedMode == "åœ°é¢åŠ é€Ÿ" and humanoid:GetState() == Enum.HumanoidStateType.Running then
            -- åœ°é¢ç§»åŠ¨åŠ é€Ÿ
            local velocity = moveDirection * (humanoid.WalkSpeed * speedMultiplier)
            if not onWall then
                root.Velocity = Vector3.new(velocity.X, root.Velocity.Y, velocity.Z)
            end
            
        elseif speedMode == "å…¨å±€åŠ é€Ÿ" then
            -- ä»»ä½•çŠ¶æ€éƒ½åŠ é€Ÿ
            local velocity = moveDirection * (humanoid.WalkSpeed * speedMultiplier)
            if not onWall then
                root.Velocity = Vector3.new(velocity.X, root.Velocity.Y, velocity.Z)
            end
            
        elseif speedMode == "ç¬ç§»å†²åˆº" then
            -- çŸ­è·ç¦»ç¬ç§»
            if (root.Position - lastPosition).Magnitude > 5 then
                local distance = math.min(10, (root.Position - lastPosition).Magnitude * 0.5)
                root.CFrame = root.CFrame + moveDirection * distance
            end
            
        elseif speedMode == "è‡ªåŠ¨è·³è·ƒ" then
            -- è‡ªåŠ¨è·³è·ƒéšœç¢
            if humanoid:GetState() == Enum.HumanoidStateType.Running then
                -- æ£€æµ‹å‰æ–¹éšœç¢
                local ray = Ray.new(root.Position + Vector3.new(0, 1, 0), root.CFrame.LookVector * 5)
                local hit, position = workspace:FindPartOnRay(ray, char)
                
                if hit and humanoid.FloorMaterial == Enum.Material.Air then
                    humanoid.Jump = true
                end
                
                -- åº”ç”¨é€Ÿåº¦
                local velocity = moveDirection * (humanoid.WalkSpeed * speedMultiplier)
                if not onWall then
                    root.Velocity = Vector3.new(velocity.X, root.Velocity.Y, velocity.Z)
                end
            end
        end
        
        -- æ›´æ–°ä½ç½®
        lastPosition = root.Position
    end
    
    -- å¯åŠ¨åŠ é€Ÿç³»ç»Ÿ
    local function startSpeedBoost()
        if speedConn then return end
        
        speedConn = game:GetService("RunService").Heartbeat:Connect(function(deltaTime)
            if not speedEnabled then return end
            
            -- åªåœ¨æŒ‰ä¸‹å¿«æ·é”®æ—¶åŠ é€Ÿ
            if UserInputService:IsKeyDown(speedKey) then
                calculateSpeed(deltaTime)
                createSpeedEffects()
            else
                cleanSpeedEffects()
            end
        end)
    end
    
    -- åœæ­¢åŠ é€Ÿç³»ç»Ÿ
    local function stopSpeedBoost()
        if speedConn then 
            speedConn:Disconnect() 
            speedConn = nil 
        end
        cleanSpeedEffects()
    end
    
    -- UI è®¾ç½®
    local speedSec = Tab4:AddSection({Name = "ç§»åŠ¨åŠ é€Ÿç³»ç»Ÿ"})
    speedSec:AddToggle({
        Name = "å¯ç”¨ç§»åŠ¨åŠ é€Ÿ",
        Default = false,
        Callback = function(v)
            speedEnabled = v
            if v then 
                startSpeedBoost()
            else 
                stopSpeedBoost()
            end
        end
    })
    
    speedSec:AddDropdown({
        Name = "åŠ é€Ÿæ¨¡å¼",
        Default = speedMode,
        Options = {"åœ°é¢åŠ é€Ÿ", "å…¨å±€åŠ é€Ÿ", "ç¬ç§»å†²åˆº", "è‡ªåŠ¨è·³è·ƒ"},
        Callback = function(v)
            speedMode = v
        end
    })
    
    speedSec:AddSlider({
        Name = "åŠ é€Ÿå€æ•°",
        Min = 1,
        Max = 5,
        Default = speedMultiplier,
        Increment = 0.1,
        ValueName = "å€",
        Callback = function(v)
            speedMultiplier = v
        end
    })
    
    speedSec:AddToggle({
        Name = "é˜²æ»‘å¢™åŠŸèƒ½",
        Default = antiSlide,
        Callback = function(v)
            antiSlide = v
        end
    })
    
    speedSec:AddToggle({
        Name = "é€Ÿåº¦ç²’å­æ•ˆæœ",
        Default = speedParticles,
        Callback = function(v)
            speedParticles = v
            if not v then
                cleanSpeedEffects()
            end
        end
    })
    
    speedSec:AddToggle({
        Name = "ç§»åŠ¨è½¨è¿¹æ•ˆæœ",
        Default = speedTrail,
        Callback = function(v)
            speedTrail = v
            if not v then
                cleanSpeedEffects()
            end
        end
    })
    
    speedSec:AddKeypicker({
        Name = "åŠ é€Ÿå¿«æ·é”®",
        Default = Enum.KeyCode.LeftShift,
        Callback = function(key)
            speedKey = key
        end
    })
    
    -- ... åé¢çš„ä»£ç ä¿æŒä¸å˜ ...
end)()

