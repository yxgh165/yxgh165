-- ⑨ NPC 透视
-----------------------------------------------------------------
local npcEspEnabled = false
local npcEspObjects = {}
local function clearNPCEsp()
    for _, o in ipairs(npcEspObjects) do o:Remove() end
    table.clear(npcEspObjects)
end
local function drawNPCEsp()
    clearNPCEsp()
    if not npcEspEnabled then return end
    for _, model in ipairs(workspace:GetDescendants()) do
        if model:IsA("Model") and model:FindFirstChildOfClass("Humanoid") and not game.Players:GetPlayerFromCharacter(model) then
            local root = model:FindFirstChild("HumanoidRootPart") or model:FindFirstChild("Torso")
            local head = model:FindFirstChild("Head")
            if not (root and head) then continue end

            local top, topVis = Camera:WorldToViewportPoint(head.Position + Vector3.new(0,1.5,0))
            local bottom, botVis = Camera:WorldToViewportPoint(root.Position - Vector3.new(0,2.5,0))
            if not (topVis and botVis) then continue end

            local h = math.abs(top.Y - bottom.Y)
            local w, l, r = h * 0.5, top.X - h * 0.25, top.X + h * 0.25
            local color = Color3.fromRGB(255, 165, 0)

            local box = Drawing.new("Quad")
            box.Visible, box.Color, box.Thickness = true, color, 1
            box.PointA, box.PointB = Vector2.new(l, top.Y), Vector2.new(r, top.Y)
            box.PointC, box.PointD = Vector2.new(r, bottom.Y), Vector2.new(l, bottom.Y)
            table.insert(npcEspObjects, box)

            local txt = Drawing.new("Text")
            txt.Visible, txt.Center, txt.Outline = true, true, true
            txt.Position = Vector2.new(top.X, top.Y - 20)
            txt.Size, txt.Color = 18, color
            txt.Text = model.Name .. " [" .. math.floor((root.Position - Camera.CFrame.Position).Magnitude) .. "m]"
            table.insert(npcEspObjects, txt)
        end
    end
end

Tab4:AddToggle({Name = "NPC 透视", Default = false, Callback = function(v)
    npcEspEnabled = v
    while npcEspEnabled do
        drawNPCEsp()
        task.wait(0.0000000000000000000000000000000000001)
    end
    clearNPCEsp()
end})

-----------------------------------------------------------------
-- ⑩ 三锁定：移速 / 跳跃 / 重力
-----------------------------------------------------------------
local lockSpeed, lockJump, lockGrav = false, false, false
local savedSpeed, savedJump, savedGrav = 16, 50, 196

local function applyValues()
    local char = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
    local hum = char:WaitForChild("Humanoid", 3)
    if lockSpeed then hum.WalkSpeed = savedSpeed end
    if lockJump then hum.JumpPower = savedJump end
    if lockGrav then workspace.Gravity = savedGrav end
end
game.Players.LocalPlayer.CharacterAdded:Connect(function()
    task.wait(.1)
    applyValues()
end)

-- 输入框 + 开关
local lockSec = Tab4:AddSection({Name = "三锁定"})
lockSec:AddTextbox({Name = "设定移速（仅保存值）", Default = tostring(savedSpeed), Callback = function(txt) savedSpeed = tonumber(txt) or 16 end})
lockSec:AddTextbox({Name = "设定跳跃高度（仅保存值）", Default = tostring(savedJump), Callback = function(txt) savedJump = tonumber(txt) or 50 end})
lockSec:AddTextbox({Name = "设定重力值（仅保存值）", Default = tostring(savedGrav), Callback = function(txt) savedGrav = tonumber(txt) or 196 end})

lockSec:AddToggle({Name = "锁定移速", Default = false, Callback = function(v) lockSpeed = v; applyValues() end})
lockSec:AddToggle({Name = "锁定跳跃", Default = false, Callback = function(v) lockJump = v; applyValues() end})
lockSec:AddToggle({Name = "锁定重力", Default = false, Callback = function(v) lockGrav = v; applyValues() end})

(function()
    -- ... 之前的代码保持不变 ...

    -----------------------------------------------------------------
    -- ⑪ 高级移动加速系统（支持躲避服务器游戏）
    -----------------------------------------------------------------
    local speedEnabled = false
    local speedMultiplier = 1.5
    local speedMode = "地面加速" -- 可选: "地面加速", "全局加速", "瞬移冲刺", "自动跳跃"
    local antiSlide = true -- 防止墙壁滑动
    local speedParticles = true -- 速度粒子效果
    local speedTrail = true -- 移动轨迹效果
    local speedKey = Enum.KeyCode.LeftShift -- 加速快捷键
    local speedConn
    local lastPosition = Vector3.new(0, 0, 0)
    
    -- 粒子效果缓存
    local particleCache = {}
    local trailCache = {}
    
    -- 速度效果创建函数
    local function createSpeedEffects()
        if not speedParticles and not speedTrail then return end
        
        local char = LocalPlayer.Character
        if not char then return end
        
        local root = char:FindFirstChild("HumanoidRootPart")
        if not root then return end
        
        -- 创建粒子效果
        if speedParticles and not particleCache[root] then
            local particles = Instance.new("ParticleEmitter")
            particles.Name = "SpeedParticles"
            particles.LightEmission = 0.8
            particles.LightInfluence = 0
            particles.Size = NumberSequence.new({
                NumberSequenceKeypoint.new(0, 0.5),
                NumberSequenceKeypoint.new(1, 0)
            })
            particles.Texture = "rbxassetid://242487987"
            particles.Transparency = NumberSequence.new(0.5)
            particles.ZOffset = 1
            particles.Acceleration = Vector3.new(0, -5, 0)
            particles.Lifetime = NumberRange.new(0.5, 1)
            particles.Rate = 50
            particles.Rotation = NumberRange.new(0, 360)
            particles.Speed = NumberRange.new(2)
            particles.VelocitySpread = 180
            particles.Parent = root
            
            particleCache[root] = particles
        end
        
        -- 创建移动轨迹
        if speedTrail and not trailCache[root] then
            local trail = Instance.new("Trail")
            trail.Name = "SpeedTrail"
            trail.FaceCamera = true
            trail.LightEmission = 0.7
            trail.Color = ColorSequence.new(Color3.fromRGB(0, 200, 255))
            trail.Transparency = NumberSequence.new(0.5)
            trail.WidthScale = NumberSequence.new(0.3)
            trail.Attachment0 = Instance.new("Attachment", root)
            trail.Attachment0.Name = "SpeedTrailAttachment0"
            trail.Attachment0.WorldPosition = root.Position
            trail.Attachment1 = Instance.new("Attachment", root)
            trail.Attachment1.Name = "SpeedTrailAttachment1"
            trail.Attachment1.WorldPosition = root.Position + Vector3.new(0, 0.5, 0)
            trail.Parent = root
            
            trailCache[root] = trail
        end
    end
    
    -- 清理效果
    local function cleanSpeedEffects()
        for _, particles in pairs(particleCache) do
            pcall(function() particles:Destroy() end)
        end
        for _, trail in pairs(trailCache) do
            pcall(function() trail:Destroy() end)
        end
        table.clear(particleCache)
        table.clear(trailCache)
    end
    
    -- 速度计算函数（带防滑墙）
    local function calculateSpeed(deltaTime)
        local char = LocalPlayer.Character
        if not char then return end
        
        local humanoid = char:FindFirstChildOfClass("Humanoid")
        local root = char:FindFirstChild("HumanoidRootPart")
        if not humanoid or not root then return end
        
        -- 检测是否在墙上（防滑墙）
        local onWall = false
        if antiSlide then
            local ray = Ray.new(root.Position, root.CFrame.LookVector * 3)
            local hit, position = workspace:FindPartOnRay(ray, char)
            if hit then
                onWall = true
            end
        end
        
        -- 计算方向向量
        local moveDirection = Vector3.new(0, 0, 0)
        if UserInputService:IsKeyDown(Enum.KeyCode.W) then
            moveDirection = moveDirection + root.CFrame.LookVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.S) then
            moveDirection = moveDirection - root.CFrame.LookVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.A) then
            moveDirection = moveDirection - root.CFrame.RightVector
        end
        if UserInputService:IsKeyDown(Enum.KeyCode.D) then
            moveDirection = moveDirection + root.CFrame.RightVector
        end
        
        -- 标准化方向向量
        if moveDirection.Magnitude > 0 then
            moveDirection = moveDirection.Unit
        end
        
        -- 应用不同加速模式
        if speedMode == "地面加速" and humanoid:GetState() == Enum.HumanoidStateType.Running then
            -- 地面移动加速
            local velocity = moveDirection * (humanoid.WalkSpeed * speedMultiplier)
            if not onWall then
                root.Velocity = Vector3.new(velocity.X, root.Velocity.Y, velocity.Z)
            end
            
        elseif speedMode == "全局加速" then
            -- 任何状态都加速
            local velocity = moveDirection * (humanoid.WalkSpeed * speedMultiplier)
            if not onWall then
                root.Velocity = Vector3.new(velocity.X, root.Velocity.Y, velocity.Z)
            end
            
        elseif speedMode == "瞬移冲刺" then
            -- 短距离瞬移
            if (root.Position - lastPosition).Magnitude > 5 then
                local distance = math.min(10, (root.Position - lastPosition).Magnitude * 0.5)
                root.CFrame = root.CFrame + moveDirection * distance
            end
            
        elseif speedMode == "自动跳跃" then
            -- 自动跳跃障碍
            if humanoid:GetState() == Enum.HumanoidStateType.Running then
                -- 检测前方障碍
                local ray = Ray.new(root.Position + Vector3.new(0, 1, 0), root.CFrame.LookVector * 5)
                local hit, position = workspace:FindPartOnRay(ray, char)
                
                if hit and humanoid.FloorMaterial == Enum.Material.Air then
                    humanoid.Jump = true
                end
                
                -- 应用速度
                local velocity = moveDirection * (humanoid.WalkSpeed * speedMultiplier)
                if not onWall then
                    root.Velocity = Vector3.new(velocity.X, root.Velocity.Y, velocity.Z)
                end
            end
        end
        
        -- 更新位置
        lastPosition = root.Position
    end
    
    -- 启动加速系统
    local function startSpeedBoost()
        if speedConn then return end
        
        speedConn = game:GetService("RunService").Heartbeat:Connect(function(deltaTime)
            if not speedEnabled then return end
            
            -- 只在按下快捷键时加速
            if UserInputService:IsKeyDown(speedKey) then
                calculateSpeed(deltaTime)
                createSpeedEffects()
            else
                cleanSpeedEffects()
            end
        end)
    end
    
    -- 停止加速系统
    local function stopSpeedBoost()
        if speedConn then 
            speedConn:Disconnect() 
            speedConn = nil 
        end
        cleanSpeedEffects()
    end
    
    -- UI 设置
    local speedSec = Tab4:AddSection({Name = "移动加速系统"})
    speedSec:AddToggle({
        Name = "启用移动加速",
        Default = false,
        Callback = function(v)
            speedEnabled = v
            if v then 
                startSpeedBoost()
            else 
                stopSpeedBoost()
            end
        end
    })
    
    speedSec:AddDropdown({
        Name = "加速模式",
        Default = speedMode,
        Options = {"地面加速", "全局加速", "瞬移冲刺", "自动跳跃"},
        Callback = function(v)
            speedMode = v
        end
    })
    
    speedSec:AddSlider({
        Name = "加速倍数",
        Min = 1,
        Max = 5,
        Default = speedMultiplier,
        Increment = 0.1,
        ValueName = "倍",
        Callback = function(v)
            speedMultiplier = v
        end
    })
    
    speedSec:AddToggle({
        Name = "防滑墙功能",
        Default = antiSlide,
        Callback = function(v)
            antiSlide = v
        end
    })
    
    speedSec:AddToggle({
        Name = "速度粒子效果",
        Default = speedParticles,
        Callback = function(v)
            speedParticles = v
            if not v then
                cleanSpeedEffects()
            end
        end
    })
    
    speedSec:AddToggle({
        Name = "移动轨迹效果",
        Default = speedTrail,
        Callback = function(v)
            speedTrail = v
            if not v then
                cleanSpeedEffects()
            end
        end
    })
    
    speedSec:AddKeypicker({
        Name = "加速快捷键",
        Default = Enum.KeyCode.LeftShift,
        Callback = function(key)
            speedKey = key
        end
    })
    
    -- ... 后面的代码保持不变 ...
end)()

